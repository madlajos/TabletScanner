{
  "name": "EagleAI",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "dev:electron": "ng-build --base-href ./ && DEBUG=true electron app.js",
    "start:electron": "ng build --base-href ./ && electron app.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "electron": "ng build && electron .",
    "electron:build": "ng build --configuration production && electron-builder",
    "packager": "electron-packager ./ --platform=win32",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "node-prune"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.0 || ^17.0.0",
    "@angular/cdk": "~16.1.1",
    "@angular/forms": "^16.0.0 || ^17.0.0",
    "@angular/material": "~16.1.1",
    "prune": "^0.0.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/animations": "^16.0.0",
    "@angular/cli": "~16.0.5",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/material": "^16.1.3",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@syncfusion/ej2-angular-dropdowns": "^22.1.35",
    "@types/chart.js": "^2.9.37",
    "@types/cross-spawn": "^6.0.2",
    "@types/jasmine": "~4.3.0",
    "chart.js": "^4.3.0",
    "chartjs-plugin-annotation": "^3.0.1",
    "cross-spawn": "^7.0.3",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng2-charts": "^4.1.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.0.2",
    "yarn": "^1.22.19"
  },
  "build": {
    "appId": "EagleAI.id",
    "asar": true,
    "compression": "maximum",
    "directories": {
      "output": "EagleAI"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/EagleEye.png"
    }
  }
}